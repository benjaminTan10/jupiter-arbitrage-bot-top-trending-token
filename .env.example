# Core Configuration
PRIVATE_KEY=your_private_key_here_protect_this_at_all_costs
SOLANA_WALLET_PRIVATE_KEY=your_private_key_here_protect_this_at_all_costs
DEFAULT_RPC=https://your-rpc-url.com
RPC_URL=https://your-rpc-url.com
ALT_RPC_LIST=https://alt-rpc-1.com,https://alt-rpc-2.com

# Token Configuration (default is WSOL if not specified)
MINT_ADDRESS=So11111111111111111111111111111111111111112

# Transaction Settings
PRIORITY=100
TX_SIMULATE=false
MAX_SLIPPAGE_PERCENT=1.0

# Trading Configuration
TRADING_ENABLED=true
WRAP_UNWRAP_SOL=true
TRADE_SIZE_SOL=1.0
