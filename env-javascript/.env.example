# Debug Settings
DEBUG=false
SKIP_INTRO=false

# UI Settings (for terminal output only)
UI_COLOR=cyan
LOG_LEVEL=info

# Trading Configuration
TRADING_ENABLED=true
WRAP_UNWRAP_SOL=true
TRADE_SIZE_SOL=1.0
MAX_SLIPPAGE_PERCENT=1.0
AUTO_RETRY_FAILED=true
RETRY_DELAY_MS=5000
MAX_RETRY_ATTEMPTS=3

# Environment Settings
NODE_ENV=production

# Security Credentials
SOLANA_WALLET_PRIVATE_KEY=PRIVATE_KEY_HERE_PROTECT_THIS_AT_ALL_COSTS

# Network Configuration
DEFAULT_RPC=https://change-this-url-in-env
ALT_RPC_LIST=https://change-this-url-in-env.gov,https://change-alt-urls-in-env.com

# Strategy Parameters
STRATEGY_TYPE=market_making
MIN_PROFIT_THRESHOLD=0.5
POSITION_LIMIT=10
AUTO_HEDGE=false

# Notification Settings
ENABLE_NOTIFICATIONS=false
NOTIFICATION_ENDPOINT=
NOTIFICATION_API_KEY=
